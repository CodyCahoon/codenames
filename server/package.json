{
    "name": "express",
    "version": "1.0.0",
    "license": "MIT",
    "main": "dist/index.js",
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "scripts": {
        "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
        "build": "tsc",
        "prestart": "yarn run build",
        "start": "node --max-old-space-size=8192 .",
        "test": "jest"
    },
    "dependencies": {
        "express": "^4.17.1",
        "htmlparser2": "^4.0.0",
        "node-fetch": "^2.6.7"
    },
    "devDependencies": {
        "@types/express": "^4.17.2",
        "@types/jest": "^24.0.24",
        "@types/node": "^12.12.21",
        "@types/node-fetch": "^2.5.4",
        "husky": "^3.1.0",
        "jest": "^24.9.0",
        "prettier": "^1.19.1",
        "pretty-quick": "^2.0.1",
        "ts-jest": "^24.2.0",
        "tslint": "^5.20.1",
        "typescript": "^3.7.3"
    },
    "prettier": {
        "useTabs": false,
        "printWidth": 100,
        "trailingComma": "all",
        "tabWidth": 4,
        "semi": true,
        "singleQuote": true,
        "bracketSpacing": true,
        "jsxBracketSameLine": true
    }
}
